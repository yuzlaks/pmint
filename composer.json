{
    "name": "pandoradev/pandoracode-mint",
    "type": "project",
    "description": "The fast mini PHP Framework",
    "keywords": [
        "framework","pandoracode"
    ],
    "homepage": "http://pandoradev.site/",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusron Laksono",
            "email": "yuzyus268@gmail.com",
